{
  "name": "drone-model-api",
  "version": "1.0.0",
  "description": "Production-ready REST API for managing drone models with CRUD operations, MongoDB, Docker, and comprehensive documentation",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "frontend": "node frontend/server.js",
    "seed": "node scripts/seed.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "echo 'Build complete'",
    "docker:build": "docker build -t drone-api:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": ["drone", "api", "rest", "mongodb", "express", "nodejs", "crud"],
  "author": "Advik-vp",
  "repository": {
    "type": "git",
    "url": "https://github.com/Advik-vp/drone-model-api.git"
  },
  "bugs": {
    "url": "https://github.com/Advik-vp/drone-model-api/issues"
  },
  "homepage": "https://github.com/Advik-vp/drone-model-api#readme",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "express-async-errors": "^3.1.1",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "cors": "^2.8.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.49.0",
    "@faker-js/faker": "^8.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
